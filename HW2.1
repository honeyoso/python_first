def backwards():
    """
    This function takes in an iterable (string, list, etc.)
    and returns its contents in reverse order
    """
    string = ()
    for x in range(len(string) - 1, 0 - 1, -1):
        return x

assert backwards('cat') == 'tac'
assert backwards([1, 2, 3]) == [3, 2, 1]
